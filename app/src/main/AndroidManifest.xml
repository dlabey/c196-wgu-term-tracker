<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.wgu.termtracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TermListActivity" />
        <activity android:name=".CourseListActivity" />
        <activity android:name=".AssessmentListActivity" />
        <activity android:name=".CourseViewActivity" />
        <activity android:name=".TermViewActivity" />
        <activity android:name=".AssessmentViewActivity" />
        <activity android:name=".AssessmentInputActivity" />
        <activity android:name=".CourseInputActivity" />
        <activity android:name=".TermInputActivity"></activity>
    </application>

</manifest>